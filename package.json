{
  "name": "apollo-fetch",
  "version": "1.0.0",
  "engines": {
    "node": "12.18.2",
    "yarn": "1.22.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cccole3/apollo-fetch"
  },
  "author": "Chris Cole",
  "license": "MIT",
  "description": "",
  "scripts": {
    "deploy": "git push heroku main",
    "logs:prod": "heroku logs --tail",
    "dev": "concurrently -k -p \"[{name}]\" -n \"Server,Client\" -c \"green.bold,cyan.bold\" \"cd server && yarn dev-server\" \"cd client && yarn start\"",
    "build:server": "cd server && yarn build",
    "build:client": "cd client && yarn build",
    "build": "concurrently -k -p \"[{name}]\" -n \"Server,Client\" -c \"green.bold,cyan.bold\" \"yarn build:server\" \"yarn build:client\"",
    "start": "cd server && yarn serve"
  },
  "devDependencies": {
    "concurrently": "^5.3.0"
  }
}
